{
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended","prettier"],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2018
    },
    "rules": {
      "linebreak-style": ["error", "unix"],
      "prettier/prettier": "error",
      "no-multiple-empty-lines":[2,{"max":0}],
      "no-console": "warn"
    },
    "plugins": ["prettier"]
  }